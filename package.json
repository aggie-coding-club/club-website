    "check": "gts check",
    "clean": "gts clean",
    "compile": "tsc -p .",
    "fix": "gts fix",
    "prepare": "npm run compile",
    "pretest": "npm run compile",
    "posttest": "npm run check",
    "start": "node build/src/index.js",
    "copy-static-assets": "ts-node copyStaticAssets.ts",
